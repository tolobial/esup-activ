<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd"
	>

	<bean
		id="beanPasswordPrincipal"
		class="org.esupportail.activfo.web.beans.BeanFieldImpl"
		scope="session"
		>

		<property name="key" value="${view.label.password.principal}"/>
		<property name="value" value=""/>
		<property name="id" value="password"/>
		<property name="validator" ref="validatorPassword"/>
		<property name="typeChamp" value=""/>
		<property name="aide" value="${view.aide.password}"/>
		<property name="converter"> <null /></property>
		
	</bean>

</beans>
