<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd"
	>

	<bean
		id="beanStudent"
		class="org.esupportail.activfo.web.beans.BeanFieldImpl"
		scope="session"
		>

		<property name="key" value="WELCOME.STATUS.STUDENT.RADIOBUTTON.LABEL"/>
		<property name="value" value="student"/>
		
		
	</bean>
	
	<bean
		id="beanPersonnel"
		class="org.esupportail.activfo.web.beans.BeanFieldImpl"
		scope="session"
		>

		<property name="key" value="WELCOME.STATUS.PERSONNEL.RADIOBUTTON.LABEL"/>
		<property name="value" value="personnel"/>
		
	</bean>
	
	
	<bean
		id="beanOldStudent"
		class="org.esupportail.activfo.web.beans.BeanFieldImpl"
		scope="session"
		>

		<property name="key" value="WELCOME.STATUS.ALUMNI.STUDENT.RADIOBUTTON.LABEL"/>
		<property name="value" value="alumni"/>
		
	</bean>
	
	<bean
		id="beanAnotherStudent"
		class="org.esupportail.activfo.web.beans.BeanFieldImpl"
		scope="session"
		>

		<property name="key" value="WELCOME.STATUS.ANOTHER.STUDENT.RADIOBUTTON.LABEL"/>
		<property name="value" value="external.student"/>
		
	</bean>

</beans>
