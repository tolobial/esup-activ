<?xml version="1.0" encoding="UTF-8" ?>

<beans 
	xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.0.xsd http://www.springframework.org/schema/aop http://www.springframework.org/schema/aop/spring-aop.xsd"
	>

	<bean
		id="beanHarpegeNumber"
		class="org.esupportail.activfo.web.beans.BeanInfoImpl"
		scope="session"
		>

		<property name="key" value="${view.label.harpegenumber}"/>
		<property name="value" value=""/>
		<property name="id" value="harpegeNumber"/>
		<property name="validator" ref="validatorHarpegeNumber"/>
		<property name="typeChamp" value=""/>
		<property name="aide" value=""/>
		<property name="converter"> <null /></property>
		
	</bean>
	
	<bean
		id="beanBirthName"
		class="org.esupportail.activfo.web.beans.BeanInfoImpl"
		scope="session"
		>

		<property name="key" value="${view.label.birthname}"/>
		<property name="value" value=""/>
		<property name="id" value="birthName"/>
		<property name="validator" ref="validatorBirthName"/>
		<property name="typeChamp" value=""/>
		<property name="aide" value=""/>
		<property name="converter"> <null /></property>
		
	</bean>
	
	<bean
		id="beanBirthDate"
		class="org.esupportail.activfo.web.beans.BeanInfoImpl"
		scope="session"
		>

		<property name="key" value="${view.label.birthdate}"/>
		<property name="value" value=""/>
		<property name="id" value="birthDate"/>
		<property name="validator" ref="validatorBirthDate"/>
		<property name="typeChamp" value=""/>
		<property name="aide" value=""/>
		<property name="converter" value="ldapDateConverter"/>
		
	</bean>
	
	

</beans>
